pipeline{
    agent any
    environment{
        PATH = "$PATH:/opt/apache-maven-3.8.2/bin"
    }
    stages{
        stage('Scan'){
            steps{
                withSonarQubeEnv(installationName: 'sonartest'){
                    sh './mvnw clean mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar'
                }
            }
        }
    }
}