pipeline{
    agent any

    stages{
        stage('Scan'){
            steps{
                withSonarQubeEnv(installationName: 'sonartest'){
                    sh 'cd demo; ./mvnw.cmd clean mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar'
                }
            }
        }
    }
}